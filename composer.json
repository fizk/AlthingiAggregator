{
    "name": "fizk/althingi-aggregator",
    "description": "Aggregate XML files from althingi.is",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": ["php", ""],

    "homepage": "https://github.com/fizk/AlthingiAggregator",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/dantleech/phly-event-dispatcher.git"
        },
        {
            "type": "git",
            "url": "https://github.com/fizk/laminas-cache.git"
        },
        {
            "type": "git",
            "url": "https://github.com/fizk/laminas-cache-storage-adapter-blackhole.git"
        },
        {
            "type": "git",
            "url": "https://github.com/fizk/laminas-cache-storage-adapter-redis.git"
        },
        {
            "type": "git",
            "url": "https://github.com/fizk/laminas-cache-storage-adapter-memory.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": ["src/","tests/"]
        }
    },
    "require": {
        "php": ">=8",
        "ext-dom": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "monolog/monolog": "^2.2",
        "laminas/laminas-diactoros": "^2.5",
        "laminas/laminas-servicemanager": "^4",
        "psr/http-server-handler": "^1.0",
        "nimbly/shuttle": "^0.5.0",
        "laminas/laminas-uri": "^2.8",
        "laminas/laminas-http": "^2.14",
        "guzzlehttp/guzzle": "^7.2",
        "phly/phly-event-dispatcher": "dev-php8",
        "laminas/laminas-cache": "^3.0",
        "laminas/laminas-cache-storage-adapter-redis": "1.1.x-dev",
        "laminas/laminas-cache-storage-adapter-blackhole": "1.1.x-dev",
        "laminas/laminas-cache-storage-adapter-memory": "2.0.x-dev",
        "nyholm/psr7": "^1.3",
        "kriswallsmith/buzz": "^1.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5"
    }
}
